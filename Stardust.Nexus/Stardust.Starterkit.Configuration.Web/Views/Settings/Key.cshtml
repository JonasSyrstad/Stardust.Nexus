
@{
    ViewBag.Title = "Key";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>Key</h2>
@using (Html.BeginForm())
{
    @Html.AntiForgeryToken()

    <div class="form-horizontal">
        <div class="form-group">
            <div class="col-md-10">stardust.configKey:</div>
            <div class="col-md-10">
                @ViewBag.Key
            </div>
            <div class="col-md-10">full config entry:</div>
            <div class="col-md-10">
                @ViewBag.AppSettings
            </div>
        </div>
    </div>
    <div class="form-group">
        <div class="col-md-offset-2 col-md-10">
            <input type="submit" value="Regenerate" class="btn btn-danger" alt="This will invalidate all stardust.configKey, all app components needs to be reconfigured" title="This will invalidate all stardust.configKey, all app components needs to be reconfigured" />
        </div>
    </div>
}
<div>
    @Html.ActionLink("Back to List", "Index")
</div>

